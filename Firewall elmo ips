select extract(year from aa.end_date) as agnio,
extract(month from aa.end_date) as mes,
extract(day from aa.end_date) as dia,
aa.parent_id, uc.first_name, max(aa.daily_count) as max_daily_count,
count(aa.daily_count) as daily_count,
sum(aa.seconds_duration)/60 as min_total
from activities_activities as aa
LEFT OUTER JOIN users_customuser as uc ON (aa.user_id = uc.id)
where  end_date between '2015-08-01' and '2015-08-20'
group by dia, mes, agnio, aa.parent_id, uc.first_name
order by agnio ASC, mes ASC, dia ASC, aa.parent_id, uc.first_name;



